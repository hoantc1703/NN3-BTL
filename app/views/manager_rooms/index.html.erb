<p id="notice"><%= notice %></p>



<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manager_rooms.each do |manager_room| %>
      <tr>
        <td><%= manager_room.first_name %></td>
        <td><%= manager_room.last_name %></td>
        <td><%= manager_room.email %></td>
        <td><%= manager_room.phone %></td>
        <td><%= manager_room.address %></td>
        <td><%= link_to 'Show', manager_room %></td>
        <td><%= link_to 'Edit', edit_manager_room_path(manager_room) %></td>
        <td><%= link_to 'Destroy', manager_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manager Room', new_manager_room_path, class:"btn btn-danger" %>

